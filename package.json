    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "standard-version": "latest",
    "style-loader": "^1.1.3",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^3.3.11"
